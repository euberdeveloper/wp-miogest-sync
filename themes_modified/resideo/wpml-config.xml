        <custom-type translate="1">agent</custom-type>
        <custom-type translate="1">lead</custom-type>
        <custom-type translate="1">testimonial</custom-type>
        <custom-type translate="1">membership</custom-type>
        <custom-type translate="1">invoice</custom-type>
