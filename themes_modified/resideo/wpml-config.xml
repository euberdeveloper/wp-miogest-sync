        <custom-field action="copy">property_size</custom-field>
        <custom-field action="copy">property_beds</custom-field>
        <custom-field action="copy">property_baths</custom-field>
        <custom-field action="copy">property_plans</custom-field>
        <custom-field action="copy">property_agent</custom-field>
